{
  "name": "autodark",
  "services": [
    {
      "type": "app",
      "data": {
        "name": "autodark",
        "source": {
          "type": "github",
          "repo": "FresHHerB/autodark",
          "branch": "main"
        },
        "build": {
          "type": "dockerfile",
          "dockerfile": "config/docker/Dockerfile"
        },
        "env": [
          {
            "name": "NODE_ENV",
            "value": "production"
          }
        ],
        "ports": [
          {
            "published": 80,
            "target": 80
          }
        ]
      }
    }
  ]
}